{
  "name": "bakedbyley-online-store",
  "version": "1.0.0",
  "description": "food ordering app built with the MERN stack",
  "main": "server.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "client-install": "npm install --prefix frontend",
    "start": "cd backend && node server.js",
    "server": "cd backend && nodemon server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lianpaulm/bakedbyley-capstone-project.git"
  },
  "author": "Lian Paul Molo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lianpaulm/bakedbyley-capstone-project/issues"
  },
  "homepage": ".",
  "dependencies": {
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.7",
    "nodemailer": "^6.7.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
